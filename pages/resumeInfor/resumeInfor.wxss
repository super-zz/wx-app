@import "../../common/css/reset.wxss";
@import "../../common/css/footer.wxss";
.j {
  padding-bottom: 117rpx;
}
.j .img {
  height: 292.5rpx;
  background-color: #18cca5;
  padding-top: 35.1rpx;
}
.j .img image {
  display: block;
  margin: 0 auto;
  width: 163.8rpx;
  height: 163.8rpx;
  border: 4rpx solid #edfbfa;
  border-radius: 50%;
  margin-bottom: 18.72rpx;
}
.j .img text {
  font-size: 35.1rpx;
  color: #fff;
  display: block;
  text-align: center;
}
.j .title {
  font-size: 35.1rpx;
  color: #333;
  line-height: 105.3rpx;
  height: 105.3rpx;
  border-bottom: 1px solid #f6f6f6;
}
.j .blank {
  width: 100%;
  height: 23.4rpx;
  background-color: #f6f6f6;
}
.j .yixiang {
  padding: 0 35.1rpx 35.1rpx 35.1rpx;
}
.j .yixiang .post {
  padding-top: 23.4rpx;
  font-size: 30.42rpx;
  color: #555;
  line-height: 46.8rpx;
  width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  overflow: hidden;
}
.j .pingjia,
.j .jiben,
.j .jingyan,
.j .liangdian,
.j .jiaoyu {
  padding: 0 35.1rpx 23.4rpx 35.1rpx;
}
.j .c {
  font-size: 30.42rpx;
  color: #555;
  line-height: 46.8rpx;
}
.j .c text {
  font-size: 30.42rpx;
  color: #dedede;
}
.j .m {
  font-size: 30.42rpx;
  color: #333;
  line-height: 46.8rpx;
}
.j .m text {
  font-size: 30.42rpx;
  color: #dedede;
}
.j .t {
  padding-top: 23.4rpx;
}
.j .liangdian .item {
  padding-top: 15rpx;
}
.j .liangdian .item text {
  display: inline-block;
  margin-right: 20rpx;
  margin-bottom: 10rpx;
  font-size: 30.42rpx;
  color: #009676;
  background-color: #defff8;
  height: 70.2rpx;
  line-height: 70.2rpx;
  padding: 0 20rpx;
  border-radius: 40rpx;
}
